{
  "name": "create-query-language",
  "private": true,
  "version": "0.0.3",
  "description": "A flexible TypeScript library for parsing and building query languages with support for lexical analysis, AST generation, and token stream processing",
  "keywords": [
    "query",
    "parser",
    "lexer",
    "ast",
    "typescript",
    "query-language",
    "token",
    "search",
    "filter",
    "expression",
    "boolean-logic",
    "parsing"
  ],
  "author": {
    "name": "Tal Kohavy",
    "email": "talkohavy@gmail.com",
    "url": "https://github.com/talkohavy"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/create-query-language.git"
  },
  "bugs": {
    "url": "https://github.com/talkohavy/create-query-language/issues"
  },
  "homepage": "https://github.com/talkohavy/create-query-language#readme",
  "main": "./index.cjs.cjs",
  "module": "./index.esm.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.esm.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "tsc": "tsc -p tsconfig.json",
    "build": "node ./build.config.mjs",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "json-easy-strip": "^1.0.6",
    "prettier": "^3.6.2",
    "rollup": "^4.49.0",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0"
  }
}
